{
  "name": "mdown-parser",
  "version": "1.0.0",
  "description": "cli tool that provides analytics on a markdown file",
  "main": "index.js",
  "scripts": {
    "test": "yarn run cypress open",
    "prettier": "prettier --no-config --write './src/**/*.{ts,js,css,json}'",
    "babel": "babel src -d lib",
    "build": "npm run prettier && npm run babel",
    "watch": "onchange 'src/**/*.js' -- npm run babel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axecopfire/mdown-parser.git"
  },
  "author": "axecopfire",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/axecopfire/mdown-parser/issues"
  },
  "homepage": "https://github.com/axecopfire/mdown-parser#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "cypress": "^3.3.0",
    "onchange": "^5.2.0",
    "prettier": "1.17.1"
  }
}
